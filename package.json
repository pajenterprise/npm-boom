{
  "version": "3.0.0",
  "name": "npm-boom",
  "description": "Generate boilerplate for es6 npm modules.",
  "main": "index.js",
  "scripts": {
    "start": "node ./lib/index.js",
    "test": "mocha --compilers js:babel-register --recursive",
    "lint": "eslint .",
    "build": "babel src -d lib",
    "prepublishOnly": "npm run build"
  },
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-register": "^6.9.0",
    "jest": "^22.0.3",
    "mocha": "^2.5.3",
    "willy": "^1.0.3"
  },
  "directories": {
    "test": "test"
  },
  "dependencies": {
    "ncp": "^2.0.0",
    "replace-in-file": "^1.0.2",
    "rimraf": "^2.5.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/reergymerej/npm-boom.git"
  },
  "keywords": [
    "es6",
    "npm",
    "boilerplate",
    "generator",
    "scaffolding"
  ],
  "preferGlobal": "true",
  "bin": {
    "npm-boom": "lib/index.js"
  },
  "author": "Jeremy Greer <jeremy.greer.atl@gmail.com>",
  "bugs": {
    "url": "https://github.com/reergymerej/npm-boom/issues"
  },
  "homepage": "https://github.com/reergymerej/npm-boom#readme"
}
